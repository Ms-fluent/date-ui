{
  "name": "@ms-fluent/date-ui",
  "version": "0.0.15",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "package": "ng-packagr -p package.json --config tsconfig.lib.json",
    "publish": "npm publish dist --access public"
  },
  "private": false,
  "ngPackage": {
    "assets": [
      "src/styles"
    ],
    "lib": {
      "entryFile": "src/components/index.ts",
      "umdModuleIds": {
        "gsap": "gsap",
        "resize-observer-polyfill": "ResizeObserver"
      }
    },
    "dest": "dist",
    "whitelistedNonPeerDependencies": [
      "@angular/animations",
      "@angular/common",
      "@angular/compiler",
      "@angular/core",
      "@angular/cdk",
      "@angular/forms",
      "@angular/platform-browser",
      "@angular/platform-browser-dynamic",
      "@angular/router",
      "rxjs",
      "tslib",
      "zone.js",
      "hammerjs",
      "gsap",
      "cleave.js",
      "resize-observer-polyfill",
      "@js-joda/core",
      "bootstrap",
      "gulp-shell",
      "moment",
      "office-ui-fabric-core"
    ]
  },
  "dependencies": {
    "@angular/animations": "~12.2.8",
    "@angular/common": "~12.2.8",
    "@angular/compiler": "~12.2.8",
    "@angular/core": "~12.2.8",
    "@angular/forms": "~12.2.8",
    "@angular/platform-browser": "~12.2.8",
    "@angular/platform-browser-dynamic": "~12.2.8",
    "@angular/router": "~12.2.8",
    "@js-joda/core": "^3.2.0",
    "bootstrap": "^5.1.0",
    "cleave.js": "^1.6.0",
    "gsap": "^3.7.1",
    "gulp-shell": "^0.8.0",
    "moment": "^2.29.1",
    "office-ui-fabric-core": "^11.0.0",
    "resize-observer-polyfill": "^1.5.1",
    "rxjs": "~6.6.0",
    "tslib": "^2.0.0",
    "zone.js": "~0.11.4"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~12.2.8",
    "@angular/cli": "~12.2.8",
    "@angular/compiler-cli": "~12.2.8",
    "@types/gsap": "^3.0.0",
    "@types/jasmine": "~3.6.0",
    "@types/node": "^12.11.1",
    "codelyzer": "^6.0.0",
    "gulp": "^4.0.2",
    "jasmine-core": "~3.6.0",
    "jasmine-spec-reporter": "~5.0.0",
    "karma": "~6.3.4",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage": "~2.0.3",
    "karma-jasmine": "~4.0.0",
    "karma-jasmine-html-reporter": "^1.5.0",
    "ng-packagr": "^12.2.2",
    "protractor": "~7.0.0",
    "ts-node": "~8.3.0",
    "tslint": "~6.1.0",
    "typescript": "~4.3.5"
  }
}